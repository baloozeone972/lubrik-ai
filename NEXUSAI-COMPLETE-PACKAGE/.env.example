# NexusAI - Environment Variables Template
# Copy this file to .env and fill in your values

# Application
VERSION=0.1.0
ENVIRONMENT=production

# Database
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=nexusai
POSTGRES_USER=nexusai

# Redis
REDIS_PASSWORD=your_redis_password_here

# JWT Security
JWT_SECRET=your_very_long_jwt_secret_key_at_least_256_bits
JWT_EXPIRATION=86400000

# AI Services
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...

# S3/MinIO Storage
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=your_minio_password_here

# Stripe Payment
STRIPE_API_KEY=sk_live_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PRICE_STANDARD=price_...
STRIPE_PRICE_PREMIUM=price_...
STRIPE_PRICE_VIP=price_...

# Frontend
FRONTEND_URL=https://nexusai.app
FRONTEND_API_URL=https://api.nexusai.app

# Monitoring
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_grafana_password_here

# Email (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
SMTP_FROM=noreply@nexusai.app

# Sentry (optional)
SENTRY_DSN=https://...@sentry.io/...

# Domain & SSL
DOMAIN_NAME=nexusai.app
SSL_EMAIL=admin@nexusai.app
